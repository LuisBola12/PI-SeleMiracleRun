// 20220621134846
// https://raw.githubusercontent.com/LuisBola12/PI-SeleMiracleRun/US-99-BorradoDeProyectoDePlanilla/FrontEnd/.eslintrc.json
{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": [
      "warn"
    ],
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "array-bracket-spacing": [
      2,
      "always"
    ],
    "space-in-parens": [
      2,
      "always"
    ],
    "object-curly-spacing": [
      2,
      "always"
    ],
    "keyword-spacing": "warn",
    "space-infix-ops": "warn"
  }
}